        <div class="animation">
            <h3 class="animate-charcter"> Graphical Representation</h3>
        </div>
        <div class="main" style="margin: 50px 0px;">
            <div class="chartBox">
                <canvas id="firstLineChart"></canvas>
            </div>
            <div class="chartBox">
                <canvas id="secondLineChart"></canvas>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

        <script>



            //setup
            const data = {
                labels: ["4-6lakhs", "6-8lakhs", "8-10lakhs", "10-12lakhs", "12 and more"],
                datasets: [{
                    label: "Number of Students",
                    data: [67, 18, 13, 2, 3],
                    backgroundColor: [
                        "rgba(255, 26, 104, 0.2)",
                        "rgba(54, 162, 235, 0.2)",
                        "rgba(255, 206, 86, 0.2)",
                        "rgba(75, 192, 192, 0.2)",
                        "rgba(153, 102, 255, 0.2)",
                        "rgba(255, 159, 64, 0.2)",
                        "rgba(0, 0, 0, 0.2)",
                    ],
                    borderColor: [
                        "rgba(255, 26, 104, 1)",
                        "rgba(54, 162, 235, 1)",
                        "rgba(255, 206, 86, 1)",
                        "rgba(75, 192, 192, 1)",
                        "rgba(153, 102, 255, 1)",
                        "rgba(255, 159, 64, 1)",
                        "rgba(0, 0, 0, 1)",
                    ],
                    borderWidth: 1
                }]
            }
            //config
            const config = {
                type: 'bar',
                data,
                options: {
                    animations: {
                        borderWidth: {
                            duration: 1000,
                            // easing: 'easeInElastic',
                            from: 3,
                            to: 1,
                            loop: true
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: true,


                        }
                    }
                }
            }
            //render bar chart
            const myChart = new Chart(document.getElementById('firstLineChart'), config);
            //set up line chart 2

            const dataSecondLine = {
                labels: ['Infosys Pvt. Ltd', 'Capgemini Pune', 'TCS Ltd', 'Adani Group', 'Adobe Company', 'Tech Mahindra Ltd.', 'Persistent System'],

                datasets: [{
                    label: 'Number of Students',
                    data: [18, 15, 5, 4, 8, 6, 10],
                    backgroundColor: [
                        "rgba(255, 26, 104, 0.2)",
                        "rgba(54, 162, 235, 0.2)",
                        "rgba(255, 206, 86, 0.2)",
                        "rgba(75, 192, 192, 0.2)",
                        "rgba(153, 102, 255, 0.2)",
                        "rgba(255, 159, 64, 0.2)",
                        "rgba(0, 0, 0, 0.2)",
                    ],
                    borderColor: [
                        "rgba(255, 26, 104, 1)",
                        "rgba(54, 162, 235, 1)",
                        "rgba(255, 206, 86, 1)",
                        "rgba(75, 192, 192, 1)",
                        "rgba(153, 102, 255, 1)",
                        "rgba(255, 159, 64, 1)",
                        "rgba(0, 0, 0, 1)",
                    ],
                    borderWidth: 1
                }]
            }
            //config lineChart
            //
            const configSecondLine = {
                type: 'line',
                data: dataSecondLine,
                options: {
                    animations: {
                        borderWidth: {
                            duration: 1000,
                            // easing: 'easeInElastic',
                            from: 3,
                            to: 1,
                            loop: true
                        }
                    },
                    scales: {
                        y: {
                            beginAtZero: true
                        }
                    }
                }
            }

            //render line chart
            const lineChart = new Chart(document.getElementById('secondLineChart'), configSecondLine);
        </script>
