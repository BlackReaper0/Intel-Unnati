    <body class='snippet-body'>

        <form action="/searchData">
            <input type="search" name="Student" id="Student" placeholder="Enter Student Name">
            <input type="search" name="Company" id="Company" placeholder="Enter Company Name">
            <input type="submit" value="search" class="btn btn-outline-primary">
        </form>
