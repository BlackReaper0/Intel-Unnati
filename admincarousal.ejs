    <style>
        .main {
            display: flex;
            justify-content: space-around;
        }

        * {
            margin: 0;
            padding: 0;
            font-family: sans-serif;
        }

        body {
            /* background: lightgray; */
            background-image: url("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTZX6zvwKt5tk2sFit0Y8xjsloLhMgVssqi1aNJ_YMZg8KU7s6UsvKX9vrGhymkGRywz0o&usqp=CAU");
            background-position: center;
            background-size: cover;
            background-repeat: no-repeat;

        }

        .chartMenu {
            width: 100vw;
            height: 40px;
            background: #1A1A1A;
            color: rgba(54, 162, 235, 1);
        }

        .chartMenu p {
            padding: 10px;
            font-size: 20px;
        }

        .chartCard {
            width: 100vw;
            height: calc(100vh - 40px);

            display: flex;
            align-items: center;
            justify-content: center;
        }

        .chartBox {
            width: 600px;
            margin: 0px 10px;
            padding: 20px;
            border-radius: 20px;
            /* border: solid 3px rgba(54, 162, 235, 1); */
            border: 7px solid grey;
            background: linear-gradient(45deg, rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.8));
        }
