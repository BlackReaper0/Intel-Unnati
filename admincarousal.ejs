                            <div class="carousel-inner">
                                <div class="carousel-item active node">
                                    <div class="row">
                                        <% for(let i=0;i<=8;i++){ %>
                                            <div class="col-md-4 mb-3 centerElement">
                                                <div class="card custom">
                                                    <div class="image">
                                                        <img class="img-fluid" alt="100%x280"
                                                            src="/images/<%-students[i].Photo  %>">
                                                    </div>
                                                    <div class="card-body">
                                                        <div class="other1">
                                                            <h4>Students:</h4>
                                                            <h4 class="card-title second"><%- students[i].Student %>
                                                            </h4>
                                                        </div>
                                                        <div class="other1">
                                                            <h4>Company :</h4>
                                                            <h4 class="card-text second"><%- students[i].Company %>
                                                            </h4>
                                                        </div>
                                                        <div class="other1">
                                                            <h4>Package :</h4>
                                                            <h4 class="card-text second"><%- students[i].Package %>
                                                            </h4>
                                                        </div>
                                                        <div class="other1">
                                                            <h4>Linkedin :</h4>
                                                            <h4 class="card-text second"><a
                                                                    href="<%- students[i].linkdein %>"><%-
                                                                        students[i].Student %></a>
                                                            </h4>
                                                        </div>
                                                        <div class="inlineCss">
                                                            <a href="delete-data?Student=<%- students[i].Student %>"
                                                                class="btn btn-outline-warning mt-3 popup">Delete
                                                                Data</a>
                                                            <a href="updateData?Student=<%- students[i].Student %>"
                                                                class="card-text btn btn-outline-warning mt-3 popup">Update
                                                                Data</a>

                                                        </div>

                                                    </div>

                                                </div>
                                            </div>
                                            <% } %>
                                    </div>
                                </div>
                                <div class="carousel-item">
                                    <div class="row">
                                        <% for(let i=9;i<=17;i++){ %>
                                            <div class="col-md-4 mb-3 centerElement">
                                                <div class="card custom">
                                                    <div class="image">
                                                        <img class="img-fluid" alt="100%x280"
                                                            src="/images/<%-students[i].Photo  %>">
