        borderColor: [
          "rgba(255, 26, 104, 1)",
          "rgba(54, 162, 235, 1)",
          "rgba(255, 206, 86, 1)",
          "rgba(75, 192, 192, 1)",
          "rgba(153, 102, 255, 1)",
          "rgba(255, 159, 64, 1)",
          "rgba(0, 0, 0, 1)",
        ],
        borderWidth: 1
      }]
    }
    //config
    const config = {
      type: 'bar',
      data,
      options: {
        animations: {
          borderWidth: {
            duration: 1000,
            // easing: 'easeInElastic',
            from: 3,
            to: 1,
            loop: true
          }
        },
        scales: {
          y: {
            beginAtZero: true,


          }
        }
      }
    }
    //render bar chart
    const myChart = new Chart(document.getElementById('firstLineChart'), config);
    //set up line chart 2

    const dataSecondLine = {
      labels: ['January', 'February', 'March', 'April', 'May', 'June'],

      datasets: [{
