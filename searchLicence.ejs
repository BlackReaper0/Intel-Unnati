        backgroundColor: [
          "rgba(255, 26, 104, 0.2)",
          "rgba(54, 162, 235, 0.2)",
          "rgba(255, 206, 86, 0.2)",
          "rgba(75, 192, 192, 0.2)",
          "rgba(153, 102, 255, 0.2)",
          "rgba(255, 159, 64, 0.2)",
          "rgba(0, 0, 0, 0.2)",
        ],
        borderColor: [
          "rgba(255, 26, 104, 1)",
          "rgba(54, 162, 235, 1)",
          "rgba(255, 206, 86, 1)",
          "rgba(75, 192, 192, 1)",
          "rgba(153, 102, 255, 1)",
          "rgba(255, 159, 64, 1)",
          "rgba(0, 0, 0, 1)",
        ],
        borderWidth: 1
      }]
    }
    //config lineChart
    //
    const configSecondLine = {
      type: 'bar',
      data: dataSecondLine,
      options: {
        animations: {
          borderWidth: {
            duration: 1000,
            // easing: 'easeInElastic',
            from: 3,
            to: 1,
            loop: true
          }
        },
        scales: {
          y: {
            beginAtZero: true
          }
        }
      }
    }

    //render line chart
    const lineChart = new Chart(document.getElementById('secondLineChart'), configSecondLine);
  </script>
